package application;

import java.io.IOException;
import java.nio.file.Files;
import java.nio.file.Path;
import java.nio.file.Paths;
import java.util.ArrayList;
import java.util.List;
import java.util.Locale;
import java.util.Scanner;
import java.util.stream.Collectors;

public class Program {

	public static void main(String[] args) throws IOException {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		List <String> pedidos = new ArrayList<>(); 
		
		pedidos.add("James Sandero; 13; 15");
		pedidos.add("Jean Green; 72; 0");
		pedidos.add("Hank McCurtis; 37; 22");
		pedidos.add("Scoot Winters; 29; 0");
		pedidos.add("James Sandero; 22; 25");
		pedidos.add("Pep Bandolero; 14; 10");
		pedidos.add("Draco Veio; 72; 0");
		pedidos.add("Kaio Comce; 11; 5");
		pedidos.add("Givanildo Paraiba; 7; 1");
		pedidos.add("Luizito; 35; 25");
		
		for (String x : pedidos) {
			System.out.println(x);
		}
		
		List<String> result = pedidos.stream().filter(x -> x.charAt(0) == 'L').collect(Collectors.toList());
		
		
		
		sc.close();

	}

}
