package application;
import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		List <String> list = new ArrayList<>(); 
		
		list.add("Maria");
		list.add("Alex");
		list.add(2, "Marco");
		list.add("Bob");
		list.add("Pedro");
		list.add("Loreta");
		list.add("Lisandra");
		
		for (String x : list) {
			System.out.println(x);
		}
		
	}
}
